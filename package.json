{
  "name": "arcgis-api-key-manager-toolkit",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/vscode",
    "packages/chrome"
  ],
  "scripts": {
    "build": "npm run build --workspace=packages/core && npm run build --workspace=packages/vscode && npm run build --workspace=packages/chrome",
    "build:core": "npm run build --workspace=packages/core",
    "build:vscode": "npm run build --workspace=packages/vscode",
    "build:chrome": "npm run build --workspace=packages/chrome",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "npm run test --workspace=packages/core && npm run test --workspace=packages/vscode"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "esbuild": "^0.25.0",
    "globals": "^15.14.0",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3"
  }
}
